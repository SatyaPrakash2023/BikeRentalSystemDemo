package com.java.bms;

public class Bike {
	private int bikeId;
	private Type type;
	private String model;
	private Status status;
	private String bikeNumber;
	public int getBikeId() {
		return bikeId;
	}
	public void setBikeId(int bikeId) {
		this.bikeId = bikeId;
	}
	public Type getType() {
		return type;
	}
	public void setType(Type type) {
		this.type = type;
	}
	public String getModel() {
		return model;
	}
	public void setModel(String model) {
		this.model = model;
	}
	public Status getStatus() {
		return status;
	}
	public void setStatus(Status status) {
		this.status = status;
	}
	public String getBikeNumber() {
		return bikeNumber;
	}
	public void setBikeNumber(String bikeNumber) {
		this.bikeNumber = bikeNumber;
	}
	public Bike(int bikeId, Type type, String model, Status status, String bikeNumber) {
		super();
		this.bikeId = bikeId;
		this.type = type;
		this.model = model;
		this.status = status;
		this.bikeNumber = bikeNumber;
	}
	public Bike() {
		super();
		// TODO Auto-generated constructor stub
	}
	@Override
	public String toString() {
		return "Bike [bikeId=" + bikeId + ", type=" + type + ", model=" + model + ", status=" + status + ", bikeNumber="
				+ bikeNumber + "]";
	}
	
	
	
}
